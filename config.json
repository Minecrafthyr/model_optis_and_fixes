{
  "name": "Variants",
  "out_dir": "out/Variants/",
  "log_level": "INFO",
  "default_merge": ["assets/res_fixes/lang/", "assets/minecraft/lang/"],
  "extra_out_dirs": ["C:/PCL2/.minecraft/resourcepacks", "C:/PCL2/.minecraft/versions/Craft to Exile 2 (VR Support)/resourcepacks", "C:/PCL2/.minecraft/versions/MITE Desperate Survival/resourcepacks", "C:/PCL2/.minecraft/versions/Shadow Fabric/resourcepacks", "C:/PCL2/.minecraft/versions/Tinker's Construct Pack/resourcepacks"],
  "tree": {
    "inputs": ["Base", "Library/Cube_Inner_Faces", "Library/Item_Offset", "Library/Shadeless"],
    "children": {
      "inputs": {
        "path": "Fixes/Lite",
        "extras": [
          "Bars_Fix",
