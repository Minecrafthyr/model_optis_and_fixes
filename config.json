          "Modern_Torches",
          "Plane_Fix",
          "Rail_Fix",
          "Random_Rotate",
          "Random_Rotate.Respackopts",
          "Shadeless_Stem",
          "Tripwire_Fix",
          {
            "path": "Mod",
            "extras": ["EBE", "EBE.Respackopts", "Farmers_Delight", "Farmers_Delight.Respackopts"]
          },
          "Respackopts"
        ]
      },
      "output": "Resource Fixes Lite",
      "children": {
        "inputs": {
          "path": "Fixes/Normal",
          "extras": ["Display", "Display.Respackopts", "Block_Variants", "Block_Variants.Respackopts", "Respackopts", "Small_Back_Faces"]
        },
        "output": "Resource Fixes",
        "children": {
          "inputs": {
            "path": "Fixes/Textured",
            "extras": ["End_Portal_Frame_Fix", "Item_Frame_Tweaks", "Misc", "Modern_Recipe_Button_Texture", "Modern_Redstone_Torch_Texture", "Modern_Spectator_GUI_Texture", "Rail_Fix", "Smooth_Stone_Slab_Fix", "Tall_Seagrass_Fix", "Respackopts"]
          },
          "output": "Resource Fixes Textured",
          "children": {
            "inputs": {
              "path": "Tweaks/Normal",
              "extras": [
                "Consistent_Planes",
                "3D_Bars",
                "3D_Ladder",
                "3D_Ladder.Respackopts",
                "3D_Pointed_Dripstone",
                "3D_Pointed_Dripstone.Respackopts",
                "3D_Redstone_Dust",
                "3D_Redstone_Dust.Respackopts",
                "Axolotl_Bucket_Variants",
                "Better_Animation",
                "Better_Copper_Grate",
                "Cross_Correction",
                "Cross_Correction.Respackopts",
                "Better_Fire",
                "Better_Leaves",
                "Better_Particles",
                "Better_Sculk_Sensor_Swing",
                "Better_Sculk_Sensor_Swing.Respackopts",
                "Better_Weather",
                "Clean_Water",
                "Display",
                "Fast_Better_Grass",
                "Fast_Better_Grass.Respackopts",
                "Language_Tweaks",
                "Lantern",
                "Mirrored_Pumpkin_Blur",
                "Misc_Blockstates",
                "Misc_Blockstates.Respackopts",
                "Moist_Farmland",
                "Moist_Farmland.Respackopts",
                "New_Torches",
                "New_Torches.Respackopts",
                "Particle_Tweaks",
                "Respackopts",
                "Sandstone_Tweaks",
                "Shadeless",
                "Shadeless.Respackopts",
                "Square_Shadow",
                "Unlit_Redstone_Ore",
                "Unlit_Redstone_Ore.Respackopts",
                "Wide_Bamboo",
                {"path": "Natural Firefly Bushes 1.0.zip", "excludes": ["pack.png", "pack.mcmeta"]},
                "Natural_Firefly_Bushes",
                {"path": "Tasty_Items-v1.2.zip", "excludes": ["credits.txt", "pack.png", "pack.mcmeta"]},
