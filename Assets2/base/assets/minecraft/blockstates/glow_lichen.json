{
  "multipart": [
    {
      "apply": {
        "model": "res_fixes:block/glow_lichen/full"
      },
      "when": {
        "east": "false",
        "north": "false",
        "south": "false",
        "up": "false",
        "west": "false",
        "down": "false"
      }
    },
    {
      "apply": {
        "model": "res_fixes:block/glow_lichen/side"
      },
      "when": {
        "north": "true"
      }
    },
    {
      "apply": {
        "model": "res_fixes:block/glow_lichen/side",
        "uvlock": true,
        "y": 90
      },
      "when": {
        "east": "true"
      }
    },
    {
      "apply": {
        "model": "res_fixes:block/glow_lichen/side",
        "uvlock": true,
        "y": 180
      },
      "when": {
        "south": "true"
      }
    },
    {
      "apply": {
        "model": "res_fixes:block/glow_lichen/side",
        "uvlock": true,
        "y": 270
      },
      "when": {
        "west": "true"
      }
    },
    {
      "apply": {
        "model": "res_fixes:block/glow_lichen/top"
      },
      "when": {
        "up": "true"
      }
    },
    {
      "apply": {
        "model": "res_fixes:block/glow_lichen/top",
        "uvlock": true,
        "x": 180
      },
      "when": {
        "down": "true"
      }
    }
  ]
}